<Page x:Class="cryptogui.AppPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="AppPage">
	<Grid>
		<DockPanel>
			<Menu DockPanel.Dock="Top">
				<MenuItem Header="_Crypto">
					<MenuItem x:Name="miLogout" Header="_Logout" Click="miLogout_Click"/>
					<MenuItem x:Name="miDelete" Header="_Delete Account" Click="miDelete_Click"/>
					<MenuItem x:Name="miExit" Header="_Exit" Click="miExit_Click"/>
				</MenuItem>
				<MenuItem Header="_About">
				</MenuItem>
			</Menu>
			<StackPanel></StackPanel>
		</DockPanel>
		<TabControl x:Name="tabcontrol" Margin="20,20,20,20" SelectionChanged="tabcontrol_SelectionChanged">
			<TabItem x:Name="itemOne" Header="Encrypt">
			</TabItem>
			<TabItem x:Name="itemTwo" Header="Decrypt">
			</TabItem>
			<TabItem x:Name="itemThree" Header="Encrypt File">
			</TabItem>
			<TabItem x:Name="itemFour" Header="Decrypt File">
			</TabItem>
			<TabItem x:Name="itemFive" Header="Steganografie">
			</TabItem>
			<TabItem x:Name="itemSix" Header="email">
			</TabItem>
		</TabControl>
	</Grid>
</Page>
